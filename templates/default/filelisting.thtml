<!-- BEGIN block_snapshot -->
    <div class="dlm_snap_tn">
        <a href="{snapshot_url}" title="{lang_click2see}"><img src="{thumbnail_url}" alt="snapshot"{xhtml}></a>
    </div>
<!-- END block_snapshot -->

<!-- BEGIN block_snaplinkicon -->
    <div class="dlm_snaplinkicon">
        <a href="{snapshot_url}" title="{lang_click2see}"><img src="{site_url}/downloads/images/snaplinkicon.png" alt=""{xhtml}></a>
